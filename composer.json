{
    "name": "ihor-radchenko/laravel-translator",
    "description": "Package for translating and transliteration from RU, UA => EN",
    "homepage": "https://github.com/ihor-radchenko/laravel-translator",
    "keywords": [
        "laravel 5",
        "translit",
        "translate"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ihor Radchenko",
            "email": "radchenko1995@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "IhorRadchenko\\LaravelTranslator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IhorRadchenko\\LaravelTranslator\\TranslatorServiceProvider"
            ],
            "aliases": {
                "Translator": "IhorRadchenko\\LaravelTranslator\\Facades\\Translator",
                "Translit": "IhorRadchenko\\LaravelTranslator\\Facades\\Transliterator"
            }
        }
    }
}
